<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title }}</title>
  <link rel="stylesheet" href="{{ 'css/bootstrap.min.css' | url }}">
  <link rel="stylesheet" href="{{ 'css/style.css' | url }}">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,100;0,400;0,700;1,100;1,400&display=swap"
    rel="stylesheet">
  <script src="https://kit.fontawesome.com/7729b7cc0d.js" crossorigin="anonymous"></script>
  
</head>
<body>
  <!-- page container -->
  <div class="container-fluid pt-5 px-5">
    <div class="row">
      <!-- Nav + SIDE NAVIGATION -->
      {% include "nav.njk" %}
      <!-- END OF SIDENAV -->

      <!-- Content Area - Container -->
      <div class="container-fluid d-flex justify-content-end px-0">
        <div class="row col-sm-12 col-md-12 col-lg-9 col-xl-10">
          <!-- top of page content-->
        {% include "home-header.njk" %}
       
          <!-- end top of page -->

          <!-- core conent area container -->
          <main id="content-container" class="container">
            <div id="page-heart" class="section">
              <div class="section ">
                <!-- full width blog -->
     
             {% include "about.njk" %}
              
                <!-- end full width blog -->

                <!-- experience section -->
                {% include "experience.njk" %}
                <!-- end experience section -->

                <!-- projects section -->
                {% include "projects.njk" %}
                <!-- end projects section -->
                <section class="row">
                  <!-- footer section -->
                  {%include "footer.njk"%}
                  <!-- end footer section -->
                </section>
              </div>
            </div>
            <!-- end core content -->
          </main>
        </div>
      </div>
    </div>
  </div>
  <!-- end container -->
  <!-- </div> -->
  


  <!-- Bootstrap JS Links -->
